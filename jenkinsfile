pipeline {
    agent any {
        stages {
            stage ("Test"){
                script {
                    sh """
                        echo "This is Test"
                    """
                }
            }
            stage ("Build") {
                script {
                    sh """
                        echo "This is Build"
                    """
                }
            }
            stage ("Deploy") {
                script {
                    sh """
                        echo "This is Deploy"
                    """
                }
            }
        }
    }
}